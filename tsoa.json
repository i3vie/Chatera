{
  "entryFile": "src/index.ts",
  "noImplicitAdditionalProperties": "silently-remove-extras",
  "controllerPathGlobs": [
    "src/controllers/*_controller.ts"
  ],
  "spec": {
    "outputDirectory": "dist",
    "specVersion": 3,
    "basePath": "/",
    "specFileBaseName": "openapi",
    "securityDefinitions": {}
  },
  "routes": {
    "routesDir": "src/generated",
    "middleware": "express"
  }
}
